    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run flutter app",
            "request": "launch",
            "type": "dart",
            "args": ["--web-port=3000"]
        },
        // These feature is not supported on flutter web yet
        //{
        //    "name": "[Guest] Run flutter web",
        //    "request": "launch",
        //    "type": "dart",
        //    "args": ["-d", "chrome", "--web-port=3000", "--route", "/guest?k=KEY_ID&t=TOKEN"]
        //},
        {
            "name": "[Guest] Run flutter",
            "request": "launch",
            "type": "dart",
            "args": ["--route", "/guest?k=KEY_ID&t=TOKEN"]
        },
        {
            "name": "Watch build runner",
            "request": "launch",
            "command": "flutter pub run build_runner watch --delete-conflicting-outputs",
            "type": "node-terminal"
        },
        {
            "name": "Run build runner",
            "request": "launch",
            "command": "flutter pub run build_runner build --delete-conflicting-outputs",
            "type": "node-terminal"
        },
        {
            "name": "condo_app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "condo_app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}